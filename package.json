{
  "name": "blockchain-test",
  "version": "1.0.0",
  "description": "Prototype Blockchain",
  "private": true,
  "scripts": {
    "start": "nodemon ./server/index.js",
    "dev:front": "react-scripts start",
    "dev:back": "nodemon ./server/index.js",
    "dev": "concurrently --kill-others \"npm run dev:back\" \"npm run dev:front\"",
    "build": "react-scripts build",
    "ganache": "ganache-cli",
    "compile": "truffle compile",
    "migration": "truffle migration --network development",
    "migration:kovan": "truffle migration --network kovan",
    "migration:reset": "truffle migration --reset --network development",
    "console": "truffle console --network kovan",
    "test": "NODE_ENV=test truffle test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "env-cmd": "^8.0.2",
    "ganache-cli": "^6.3.0",
    "openzeppelin-solidity": "^2.3.0",
    "polished": "^2.3.3",
    "truffle": "5.0.4",
    "truffle-contract": "^4.0.9",
    "truffle-privatekey-provider": "1.1.0",
    "use-force-update": "^1.0.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.10"
  }
}
